{
    "name": "@snailicide/g-windi",
    "version": "0.0.3",
    "description": "Function library for windicss",
    "private": false,
    "type": "module",
    "files": [
        "docs",
        "types",
        "dist",
        "CHANGELOG.md",
        "exampleNodeScript.ts"
    ],
    "exports": {
        ".": [
            {
                "import": "./dist/index.mjs",
                "types": "./types/index.d.ts",
                "require": "./dist/index.cjs",
                "default": "./dist/index.js"
            },
            "./dist/index.js"
        ]
    },
    "main": "dist/index.js",
    "types": "types/index.d.ts",
    "umd:main": "dist/index.umd.js",
    "jsdelivr": "dist/index.umd.js",
    "unpkg": "dist/index.umd.js",
    "module": "dist/index.esm.js",
    "cdn": "dist/index.umd.js",
    "keywords": [
        "typescript",
        "js",
        "ts",
        "javascript",
        "function library",
        "type library",
        "typeguards",
        "typeguard",
        "utilities"
    ],
    "scripts": {
        "\n========== BUILD >> ==========": "",
        "build": "pnpm build:ts && pnpm build:rollup",
        "build:rollup": "rollup --config ./rollup.config.js",
        "build:ts": "tsc --build",
        "\n========== DEV >> ==========": "",
        "dev": "tsc --build --watch",
        "\n========== TEST >> ==========": "",
        "test": "vitest run",
        "test:watch": "vitest watch",
        "\n========== DOCS >> ==========": "",
        "docs": "",
        "\n========== PUBLISH >> ==========": "",
        "release": "pnpm prerelease && pnpm publish --access public --report-summary --no-git-checks",
        "prerelease": "pnpm clean && pnpm build && pnpm fix && pnpm check && pnpm test",
        "\n========== CLEAN >> ==========": "",
        "clean": "pnpm clean:ts && pnpm clean:build",
        "clean:ts": "tsc --build --clean",
        "clean:build": "rm -R -f dist && rm -R -f types && rm -R -f docs"
    },
    "homepage": "https://github.com/gbtunney/snailicide-monorepo#readme",
    "bugs": {
        "url": "https://github.com/gbtunney/snailicide-monorepo/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/gbtunney/snailicide-monorepo.git"
    },
    "license": "MIT",
    "author": {
        "name": "Gillian Tunney",
        "email": "gbtunney@mac.com"
    },
    "directories": {
        "doc": "docs"
    },
    "dependencies": {
        "@snailicide/g-library": "workspace:^",
        "@types/ramda": "^0.29.4",
        "@vueuse/core": "^10.4.1",
        "chroma.ts": "^1.0.10",
        "ramda": "^0.29.0",
        "ramda-adjunct": "^4.1.1",
        "type-fest": "^4.3.1",
        "vue": "^3.3.4",
        "windicss": "^3.5.6"
    },
    "devDependencies": {
        "@snailicide/build-config": "workspace:^",
        "@types/node": "^20.6.2",
        "eslint": "^8.49.0",
        "jsdoc": "^4.0.2",
        "prettier": "^3.0.3",
        "rollup": "^3.29.2",
        "rollup-plugin-typescript2": "^0.35.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.2.2",
        "vitest": "^1.6.0"
    }
}
